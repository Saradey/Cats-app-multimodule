<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cnlContainerCathedCats"
    style="@style/BackgroundClickAndFocusTrue.BackgroundRipple"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/txvTitleName"
        style="@style/ColorTextDescription.LargeTextDescription"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/size_dp_16"
        android:layout_marginTop="@dimen/size_dp_12"
        android:layout_marginBottom="@dimen/size_dp_12"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Abyssinian" />

    <TextView
        android:id="@+id/txvIdTitle"
        style="@style/ColorTextDescription.LargeTextDescription"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/size_dp_12"
        android:layout_marginEnd="@dimen/size_dp_12"
        android:layout_marginBottom="@dimen/size_dp_12"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Aby" />
</androidx.constraintlayout.widget.ConstraintLayout>